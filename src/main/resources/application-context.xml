<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
default-autowire="byName">


    <bean id="main" class="HW3.Main"/>

    <bean id="daoImp" class="HW3.DAO.hibernate.DAOImp"/>

    <bean id="companiesController" class="HW3.Controllers.CompaniesController"/>
    <bean id="companiesDAO" class="HW3.DAO.hibernate.HCompaniesDAO"/>

    <bean id="customersController" class="HW3.Controllers.CustomersController"/>
    <bean id="customersDAO" class="HW3.DAO.hibernate.HCustomersDAO"/>

    <bean id="developersController" class="HW3.Controllers.DevelopersController"/>
    <bean id="developersDAO" class="HW3.DAO.hibernate.HDevelopersDAO"/>

    <bean id="projectsController" class="HW3.Controllers.ProjectsController"/>
    <bean id="projectsDAO" class="HW3.DAO.hibernate.HProjectsDAO"/>

    <bean id="skillsController" class="HW3.Controllers.SkillsController"/>
    <bean id="skillsDAO" class="HW3.DAO.hibernate.HSkillsDAO"/>

</beans>